{
	"name": "dekode/ninjaforms-azure",
	"description": "Dekode WordPress boilerplate with modern development tools, easier configuration, and an improved folder structure",
	"type": "wordpress-plugin",
	"version": "1.0.0",
	"license": "GPL-3.0-or-later",
	"prefer-stable": true,
	"minimum-stability": "dev",
	"repositories": {
		"wpackagist": {
			"type": "composer",
			"url": "https://wpackagist.org"
		},
		"dekode-packages": {
			"type": "composer",
			"url": "https://packages.dekodes.no"
		}
	},
	"require": {
		"php": ">=7.2",
		"composer/installers": "~1.11.0"
	},
	"require-dev": {
		"dekode/coding-standards": "4.0.0",
		"dekode/logging": "@dev"
	},
	"scripts": {
		"post-install-cmd": [

		],
		"lint": [
			"./vendor/bin/phpcs ."
		],
		"build": [
			"@composer install"
		]
	}
}
